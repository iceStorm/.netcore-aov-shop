<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Colorlib Templates">
    <meta name="author" content="Colorlib">
    <meta name="keywords" content="Colorlib Templates">

    <!-- Title Page-->
    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/x-icon" href="~/fav-icon.png" />


    <!-- Icons font CSS-->
    <link href="~/templates/colorlib-regform-4/vendor/mdi-font/css/material-design-iconic-font.css" rel="stylesheet" />
    <link href="~/templates/colorlib-regform-4/vendor/font-awesome-4.7/css/font-awesome.css" rel="stylesheet" />

    <!-- Font special for pages-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Vendor CSS-->
    <link href="~/templates/colorlib-regform-4/vendor/select2/select2.min.css" rel="stylesheet" />
    <link href="~/templates/colorlib-regform-4/vendor/datepicker/daterangepicker.css" rel="stylesheet" />

    <!-- Main CSS-->
    <link href="~/templates/colorlib-regform-4/css/main.css" rel="stylesheet" />

    <!-- SnackBar CSS-->
    <link href="~/libs/snackbar-notifier/css/snackbar.css" rel="stylesheet" />
</head>

<body>

    @RenderBody();


    <!-- Jquery JS-->
    <script src="~/templates/colorlib-regform-4/vendor/jquery/jquery.js"></script>

    <!-- Vendor JS-->
    <script src="~/templates/colorlib-regform-4/vendor/select2/select2.min.js"></script>
    <script src="~/templates/colorlib-regform-4/vendor/datepicker/moment.min.js"></script>
    <script src="~/templates/colorlib-regform-4/vendor/datepicker/daterangepicker.js"></script>

    <!-- Main JS-->
    <script src="~/templates/colorlib-regform-4/js/global.js"></script>
    <script src="~/libs/snackbar-notifier/js/snackbar.js"></script>


    @if (TempData["message"] != null)
    {
        <snackbar></snackbar>
        <script>
            const snackbar = new SnackBar;

            snackbar.make("message", [
                "@TempData["message"]", null, "bottom", "center"
            ], 5000);
        </script>
    }
</body>
</html>